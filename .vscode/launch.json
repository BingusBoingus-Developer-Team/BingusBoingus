    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "start:dev",
            "runtimeExecutable": "npm",
            "console": "integratedTerminal",
            "preLaunchTask": "npm: build",
            "envFile": "${workspaceFolder}/.env",
            "runtimeArgs": [
                "run-script",
                "start"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "start:dev:Lukas",
            "runtimeExecutable": "npm",
            "console": "integratedTerminal",
            "preLaunchTask": "npm: build",
            "envFile": "${workspaceFolder}/.env.Lukas",
            "runtimeArgs": [
                "run-script",
                "start"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "deploy-commands:dev:Lukas",
            "runtimeExecutable": "npm",
            "console": "integratedTerminal",
            "preLaunchTask": "npm: build",
            "envFile": "${workspaceFolder}/.env.Lukas",
            "runtimeArgs": [
                "run-script",
                "deploy-commands"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "deploy-commands:dev:",
            "runtimeExecutable": "npm",
            "console": "integratedTerminal",
            "preLaunchTask": "npm: build",
            "envFile": "${workspaceFolder}/.env",
            "runtimeArgs": [
                "run-script",
                "deploy-commands"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
    ]
}