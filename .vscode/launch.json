  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "start",
      "runtimeExecutable": "npm",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "runtimeArgs": ["run-script", "start:debug"],
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "start:dev",
      "runtimeExecutable": "npm",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env.dev",
      "runtimeArgs": ["run-script", "start:debug"],
      "skipFiles": ["<node_internals>/**"]
    }
  ]

}
